angstrom = "Ã…"

for model in [xstal_model, mm_model]:
    print(f"Residues within 3 {angstrom} of EQU in the {model.name} coordinates:")
    wat_count = 0
    AA_count = 0
    for res in model.cutoff_3.residues:
        if res.resname == 'WAT':
            wat_count += 1
            continue
        AA_count += 1
        print(f"\t{res.resname} {res.resid}")
    print(f"\tAmino acid: {AA_count}")
    print(f"\tWater count: {wat_count}\n")






